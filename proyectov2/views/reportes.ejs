<!doctype html>
<html ng-app="app">
    <head>
        <title>AngularJS Website</title>
        <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet">
        <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
        <link rel="stylesheet" href="/stylesheets/css/bootstrap.min.css" type="text/css">
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
        <link href='https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
        <link rel="stylesheet" href="/stylesheets/font-awesome/css/font-awesome.min.css" type="text/css">
        <link rel="stylesheet" href="/stylesheets/css/magnific-popup.css" type="text/css">
        <link rel="stylesheet" href="/stylesheets/css/creative.css" type="text/css">
        <script type="text/javascript" src="../fusioncharts/js/fusioncharts.js"></script>
        <script type="text/javascript" src="../fusioncharts/js/themes/fusioncharts.theme.fint.js"></script>
        <script type="text/javascript">
          FusionCharts.ready(function() {
            var revenueChart = new FusionCharts({
                "type": "column2d",
                "renderAt": "chartContainer",
                "width": "500",
                "height": "300",
                "dataFormat": "json",
                "dataSource":  {
                  "chart": {
                    "caption": "Reservaciones por mes",
                    "subCaption": "Hoteles Aileen",
                    "xAxisName": "Mes",
                    "yAxisName": "Número de reservaciones",
                    "theme": "fint"
                 },
                 "data": [
                    {
                       "label": "Jan",
                       "value": "<%= ene %>"
                    },
                    {
                       "label": "Feb",
                       "value": "<%= feb %>"
                    },
                    {
                       "label": "Mar",
                       "value": "<%= mar %>"
                    },
                    {
                       "label": "Apr",
                       "value": "<%= abr %>"
                    },
                    {
                       "label": "May",
                       "value": "<%= may %>"
                    },
                    {
                       "label": "Jun",
                       "value": "<%= jun %>"
                    },
                    {
                       "label": "Jul",
                       "value": "<%= jul %>"
                    },
                    {
                       "label": "Aug",
                       "value": "<%= ago %>"
                    },
                    {
                       "label": "Sep",
                       "value": "<%= sep %>"
                    },
                    {
                       "label": "Oct",
                       "value": "<%= oct %>"
                    },
                    {
                       "label": "Nov",
                       "value": "<%= nov %>"
                    },
                    {
                       "label": "Dic",
                       "value": "<%= dic %>"
                    }
                  ]
              }
          });
          var revenueChart1 = new FusionCharts({
                "type": "column2d",
                "renderAt": "chartContainer1",
                "width": "500",
                "height": "300",
                "dataFormat": "json",
                "dataSource":  {
                  "chart": {
                    "caption": "Reservaciones por tipo de habitación",
                    "subCaption": "Hoteles Aileen",
                    "xAxisName": "Mes",
                    "yAxisName": "Número de habitaciones reservadas",
                    "theme": "fint"
                 },
                 "data": [
                    {
                       "label": "Habitación Simple",
                       "value": "<%= simple %>"
                    },
                    {
                       "label": "Habitación Doble",
                       "value": "<%= doble %>"
                    },
                    {
                       "label": "Habitación Premier",
                       "value": "<%= premier %>"
                    }
                  ]
              }
          });
          var revenueChart2 = new FusionCharts({
                "type": 'doughnut2d',
                "renderAt": 'chartContainer3',
                "width": "500",
                "height": "300",
                "dataFormat": 'json',
                "dataSource": {
                    "chart": {
                        "caption": "Split of Revenue by Product Categories",
                        "subCaption": "Last year",
                        "numberPrefix": "$",
                        "paletteColors": "#f45b00, #1aaf5d, #0075c2",
                        "bgColor": "#ffffff",
                        "showBorder": "0",
                        "use3DLighting": "0",
                        "showShadow": "0",
                        "enableSmartLabels": "0",
                        "startingAngle": "310",
                        "showLabels": "0",
                        "showPercentValues": "1",
                        "showLegend": "1",
                        "legendShadow": "0",
                        "legendBorderAlpha": "0",
                        "defaultCenterLabel": "Total revenue: $64.08K",
                        "centerLabel": "Revenue from $label: $value",
                        "centerLabelBold": "1",
                        "showTooltip": "0",
                        "decimals": "0",
                        "captionFontSize": "14",
                        "subcaptionFontSize": "14",
                        "subcaptionFontBold": "0"
                    },
                    "data": [
                        {
                            "label": "Alta",
                            "value": "<%= alta %>"
                        }, 
                        {
                            "label": "Media",
                            "value": "<%= media %>"
                        }, 
                        {
                            "label": "Baja",
                            "value": "<%= baja %>"
                        }
                    ]
                }
            });
        revenueChart.render();
        revenueChart1.render();
        revenueChart2.render();
        })
        </script>
    </head>
    <body>
        <nav id="mainNav" class="navbar navbar-default navbar-fixed-top">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand page-scroll" href="/">Hoteles Aileen</a>
                </div>
    
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a class="page-scroll" href="/indexAdmin#/altaHoteles">Alta de Hoteles</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/indexAdmin#/listaHoteles">Lista de Hoteles</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/indexAdmin#/listaUsuarios">Lista de Usuarios</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/indexAdmin#/altaAdmin">Alta Administradores</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/indexAdmin#/listaAdmin">Lista Administradores</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/indexAdmin#/reservaciones">Reservaciones</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/indexAdmin#/comentariosAdmin">Comentarios</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/altaTarjeta">Reportes</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="/">Salir</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <section id="services">
            <div class="container">
                <table align="center">
                    <tr>
                        <td>
                            <div id="chartContainer">FusionCharts XT will load here!</div>
                        </td>
                        <td>
                            <div id="chartContainer1">FusionCharts XT will load here!</div>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="2" align="center">
                            <div id="chartContainer3">FusionCharts XT will load here!</div>
                        </td>
                    </tr>
                </table>
                <br>
                <table align="center">
                    <tr>
                        <td align="center">
                            <hr/>
                        </td>
                    </tr>
                    <tr>
                        <td align="center">
                            <pie-pagina></pie-pagina>
                        </td>
                    </tr>
                    <tr>
                        <td align="center">
                            <hr/>
                        </td>
                    </tr>
                </table>
            </div>
        </section>
        
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.0-rc.1/angular.min.js"></script>
        <script src="https://code.angularjs.org/1.3.0-rc.1/angular-route.min.js"></script>
        <script type="text/javascript" src="/js/app.js"></script>
        <script type="text/javascript" src="/js/controllers.js"></script>
        <script src="/stylesheets/js/jquery.js"></script>
        <script src="/stylesheets/js/bootstrap.min.js"></script>
        <script src="/stylesheets/js/scrollreveal.min.js"></script>
        <script src="/stylesheets/js/jquery.easing.min.js"></script>
        <script src="/stylesheets/js/jquery.fittext.js"></script>
        <script src="/stylesheets/js/jquery.magnific-popup.min.js"></script>
        <script src="/stylesheets/js/creative.js"></script>
</html>